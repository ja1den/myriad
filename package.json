{
	"name": "myriad",
	"version": "1.0.0",
	"private": true,
	"homepage": "https://github.com/ja1den/myriad#readme",
	"bugs": {
		"url": "https://github.com/ja1den/myriad/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ja1den/myriad.git"
	},
	"license": "MIT",
	"author": "Jaiden Douglas",
	"scripts": {
		"commit": "cz",
		"postinstall": "husky install",
		"lint": "sort-package-json && prettier -wu \"*\" && eslint \"**/*.ts\" --fix",
		"prod": "ts-node -r dotenv/config src/index.ts",
		"start": "nodemon"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*": "prettier -wu",
		"*.ts": "eslint --fix",
		"package.json": "sort-package-json"
	},
	"dependencies": {
		"colors": "^1.4.0",
		"discord.js": "^12.5.1",
		"dotenv": "^8.2.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@types/node": "^14.14.10",
		"@typescript-eslint/eslint-plugin": "^4.8.2",
		"@typescript-eslint/parser": "^4.8.2",
		"commitizen": "^4.2.2",
		"eslint": "^7.14.0",
		"husky": "^5.0.4",
		"lint-staged": "^10.5.2",
		"nodemon": "^2.0.6",
		"prettier": "^2.2.1",
		"sort-package-json": "^1.48.0",
		"ts-node": "^9.0.0",
		"typescript": "^4.1.2"
	}
}
